
public class sumOfEvenindex {

    static int SumOf_evenIndex(int arr[]) {
        int sum = 0;
        for (int i = 0; i < arr.length; i += 2) {
            if (i % 2 == 0)
                sum = sum + arr[i];
        }

        return sum;
    }

    public static void main(String[] args) {

        int arr[] = { 1, 4, 3, 5, 7, 9, 10 };

        int sum = SumOf_evenIndex(arr);
        System.out.println(sum);
    }

}
